{
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "client/**/*", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ]
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
